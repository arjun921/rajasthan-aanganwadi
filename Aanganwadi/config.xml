<?xml version='1.0' encoding='utf-8'?>
<widget id="com.arjoonn.aanganwadi" version="1.4.36" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Aanganwadi</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <author email="arjoonn.94@gmail.com" href="http://arjoonn.com">
        Arjoonn Sharma
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3500" />
